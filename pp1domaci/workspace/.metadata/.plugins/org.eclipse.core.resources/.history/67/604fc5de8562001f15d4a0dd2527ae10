program EqualDeclaration
	const int nula = 0;
	const int jedan = 1;
	const int pet = 5;
	

	int niz[];
	int mat[][];
{
    void main()
    	int bodovi;
    {
    	mat = new int[2][3];
		mat[nula][jedan - 1] = jedan;
		mat[0][mat[0][0]] = 2;
		mat[0][mat[0][1]] = 5;
		mat[1][mat[0][0]*2] = 4;
		mat[mat[0][0]][1] = 3;
		mat[1][0] = mat[0][0] + 2 * mat[0][0];
		
		print(mat[1][2]);
		print(mat[0][2]);

    }
}